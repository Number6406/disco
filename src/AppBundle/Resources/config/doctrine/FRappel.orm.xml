<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\FRappel" table="f_rappel">
    <indexes>
      <index name="user_champ" columns="user,champ"/>
    </indexes>
    <id name="dbkey" type="integer" column="dbkey">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="user" type="string" column="user" length="45" nullable="false"/>
    <field name="champ" type="string" column="champ" length="45" nullable="false"/>
    <field name="valeur" type="string" column="valeur" length="45" nullable="false"/>
  </entity>
</doctrine-mapping>
