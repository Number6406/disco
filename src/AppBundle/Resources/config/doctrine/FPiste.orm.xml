<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\FPiste" table="f_piste">
    <indexes>
      <index name="cd" columns="cd"/>
      <index name="artiste" columns="artiste"/>
      <index name="piste" columns="cd,disque,piste"/>
    </indexes>
    <id name="dbkey" type="integer" column="dbkey">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="piste" type="integer" column="piste" nullable="false"/>
    <field name="cd" type="integer" column="cd" nullable="false"/>
    <field name="disque" type="integer" column="disque" nullable="false"/>
    <field name="titre" type="string" column="titre" length="300" nullable="false"/>
    <field name="artiste" type="integer" column="artiste" nullable="false"/>
    <field name="langue" type="integer" column="langue" nullable="false"/>
    <field name="anim" type="boolean" column="anim" nullable="false"/>
    <field name="paulo" type="boolean" column="paulo" nullable="false"/>
    <field name="star" type="boolean" column="star" nullable="false"/>
  </entity>
</doctrine-mapping>
