<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ForumProgra" table="forum_progra">
    <indexes>
      <index name="author" columns="author"/>
      <index name="userid" columns="userid"/>
      <index name="datestamp" columns="datestamp"/>
      <index name="subject" columns="subject"/>
      <index name="thread" columns="thread"/>
      <index name="parent" columns="parent"/>
      <index name="approved" columns="approved"/>
      <index name="msgid" columns="msgid"/>
      <index name="modifystamp" columns="modifystamp"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="datestamp" type="datetime" column="datestamp" nullable="false"/>
    <field name="thread" type="integer" column="thread" nullable="false"/>
    <field name="parent" type="integer" column="parent" nullable="false"/>
    <field name="author" type="string" column="author" length="37" nullable="false"/>
    <field name="subject" type="string" column="subject" length="255" nullable="false"/>
    <field name="email" type="string" column="email" length="200" nullable="false"/>
    <field name="host" type="string" column="host" length="255" nullable="false"/>
    <field name="emailReply" type="string" column="email_reply" length="1" nullable="false"/>
    <field name="approved" type="string" column="approved" length="1" nullable="false"/>
    <field name="msgid" type="string" column="msgid" length="100" nullable="false"/>
    <field name="modifystamp" type="integer" column="modifystamp" nullable="false"/>
    <field name="userid" type="integer" column="userid" nullable="false"/>
    <field name="closed" type="boolean" column="closed" nullable="false"/>
    <field name="idDisco" type="integer" column="id_disco" nullable="false"/>
  </entity>
</doctrine-mapping>
