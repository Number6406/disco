<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\FUser" table="f_user">
    <indexes>
      <index name="user" columns="user"/>
    </indexes>
    <id name="dbkey" type="integer" column="dbkey">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="user" type="integer" column="user" nullable="false"/>
    <field name="suppr" type="boolean" column="suppr" nullable="false"/>
    <field name="inactif" type="boolean" column="inactif" nullable="false"/>
    <field name="nom" type="string" column="nom" length="200" nullable="false"/>
    <field name="prenom" type="string" column="prenom" length="200" nullable="false"/>
    <field name="cotisation" type="boolean" column="cotisation" nullable="false"/>
    <field name="groupe" type="string" column="groupe" length="45" nullable="false"/>
    <field name="emission" type="string" column="emission" length="200" nullable="false"/>
    <field name="email" type="string" column="email" length="100" nullable="false"/>
    <field name="libelle" type="string" column="libelle" length="400" nullable="false"/>
    <field name="pwd" type="string" column="pwd" length="20" nullable="false"/>
    <field name="login" type="string" column="login" length="45" nullable="false"/>
    <field name="alerteProgra" type="boolean" column="alerte_progra" nullable="false"/>
  </entity>
</doctrine-mapping>
