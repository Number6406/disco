<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\FGenre" table="f_genre">
    <indexes>
      <index name="libelle" columns="libelle"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="genre" columns="genre"/>
    </unique-constraints>
    <id name="dbkey" type="integer" column="dbkey">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="genre" type="integer" column="genre" nullable="false"/>
    <field name="libelle" type="string" column="libelle" length="45" nullable="false"/>
    <field name="actif" type="boolean" column="actif" nullable="false"/>
  </entity>
</doctrine-mapping>
