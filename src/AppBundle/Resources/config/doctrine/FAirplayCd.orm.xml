<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\FAirplayCd" table="f_airplay_cd">
    <indexes>
      <index name="airplay" columns="airplay"/>
      <index name="cd" columns="cd"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="cle" columns="airplay,cd"/>
    </unique-constraints>
    <id name="dbkey" type="integer" column="dbkey">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="airplay" type="integer" column="airplay" nullable="false"/>
    <field name="cd" type="integer" column="cd" nullable="false"/>
    <field name="ordre" type="integer" column="ordre" nullable="false"/>
  </entity>
</doctrine-mapping>
