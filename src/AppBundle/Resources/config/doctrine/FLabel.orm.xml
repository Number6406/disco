<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\FLabel" table="f_label">
    <indexes>
      <index name="label" columns="label"/>
      <index name="libelle" columns="libelle"/>
    </indexes>
    <id name="dbkey" type="integer" column="dbkey">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="label" type="integer" column="label" nullable="false"/>
    <field name="libelle" type="string" column="libelle" length="45" nullable="false"/>
    <field name="email" type="string" column="email" length="45" nullable="false"/>
    <field name="telephone" type="string" column="telephone" length="45" nullable="false"/>
    <field name="adresse" type="string" column="adresse" length="45" nullable="false"/>
    <field name="adresse2" type="string" column="adresse2" length="45" nullable="false"/>
    <field name="cp" type="string" column="cp" length="45" nullable="false"/>
    <field name="ville" type="string" column="ville" length="45" nullable="false"/>
    <field name="mailProgra" type="string" column="mail_progra" length="250" nullable="false"/>
    <field name="contact1" type="string" column="contact1" length="150" nullable="false"/>
    <field name="siteweb" type="string" column="siteweb" length="150" nullable="false"/>
    <field name="suppr" type="boolean" column="suppr" nullable="false"/>
    <field name="info" type="text" column="info" nullable="false"/>
  </entity>
</doctrine-mapping>
