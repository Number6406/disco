<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\FConfig" table="f_config">
    <indexes>
      <index name="cle" columns="cle"/>
    </indexes>
    <id name="dbkey" type="integer" column="dbkey">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cle" type="string" column="cle" length="50" nullable="false"/>
    <field name="valeur" type="string" column="valeur" length="500" nullable="false"/>
    <field name="user" type="integer" column="user" nullable="false"/>
  </entity>
</doctrine-mapping>
